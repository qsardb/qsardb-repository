<module>
	<source
		path="gwt"
	/>

	<public
		path="public"
	/>

	<inherits
		name="org.dspace.qsardb.rpc.Gwt"
	/>

	<inherits
		name="com.google.gwt.core.Core"
	/>
	<inherits
		name="com.google.gwt.http.HTTP"
	/>
	<inherits
		name="com.google.gwt.json.JSON"
	/>
	<inherits
		name="org.fusesource.restygwt.RestyGWT"
	/>
	<inherits
		name="com.google.gwt.i18n.I18N"
	/>
	<inherits 
		name="com.google.gwt.user.User"
	/>

	<inherits
		name="com.google.gwt.user.theme.standard.Standard"
	/>

	<stylesheet
		src="application.css"
	/>

	<inherits
		name="com.googlecode.gflot.GFlot"
	/>

	<inherits
		name="com.kiouri.sliderbar.SliderBarMVP"
	/>

	<inherits
		name="com.reveregroup.gwt.imagepreloader.ImagePreloader"
	/>

	<set-property
		name="gflot.load.flot.image"
		value="DISABLED"
	/>
	<set-property
		name="gflot.load.flot.pie"
		value="DISABLED"
	/>
	<set-property
		name="gflot.load.flot.selection"
		value="DISABLED"
	/>
	<set-property
		name="gflot.load.flot.symbol"
		value="DISABLED"
	/>
</module>
