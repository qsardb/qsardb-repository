				<repeatable>true</repeatable>
				<label>Species</label>
				<input-type>onebox</input-type>
				<hint>Binomial name</hint>
				<required></required>
			</field>
			<field>
				<dc-schema>qdb</dc-schema>
				<dc-element>descriptor</dc-element>
				<dc-qualifier>application</dc-qualifier>
				<repeatable>true</repeatable>
				<label>Descriptor calculation software</label>
				<input-type>onebox</input-type>
				<hint>The name of the application</hint>
				<required></required>
			</field>
			<field>
				<dc-schema>qdb</dc-schema>
				<dc-element>prediction</dc-element>
				<dc-qualifier>application</dc-qualifier>
				<repeatable>true</repeatable>
				<label>Prediction training or validation software</label>
				<input-type>onebox</input-type>
				<hint>The name of the application</hint>
				<required></required>
			</field>
			<field>
				<dc-schema>qdb</dc-schema>
				<dc-element>model</dc-element>
				<dc-qualifier>type</dc-qualifier>
				<repeatable>true</repeatable>
				<label>Model type</label>
				<input-type>onebox</input-type>
				<hint>The type of the statistical or data mining model</hint>
				<required></required>
			</field>
		</page>
