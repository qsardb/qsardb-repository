				<repeatable>true</repeatable>
				<label>QMRF endpoint</label>
				<input-type value-pairs-name="qdb_endpoints">dropdown</input-type>
				<hint>QMRF endpoint</hint>
				<required>You must enter a QMRF endpoint</required>
